@model Dalgucci.ViewModel.Manager_LoginViewModel

<h1>로그인</h1>
<form class=" 1 " form-horizontal" method="post" asp-controller="Account" asp-action="Manager_Login">
    <div class="text-danger" asp-validation-summary="ModelOnly"></div>
    <div class="form-group">
        <label>아이디</label>
        <input type="text" asp-for="Manager_ID" class="form-control" placeholder="아이디 입력해" />
        <span class="text-danger" asp-validation-for="Manager_ID"></span>
    </div>

    <div class="form-group">
        <label>비밀번호</label>
        <input type="text" asp-for="Manager_Pwd" class="form-control" placeholder="비밀번호 입력해" />
        <span class="text-danger" asp-validation-for="Manager_Pwd"></span>
    </div>

    <div class="form-group">
        <button type="submit" class="btn btn-success">로그인</button>
    </div>
</form>